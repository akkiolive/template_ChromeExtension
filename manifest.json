{
    "manifest_version": 3,
    "name": "My Extension",
    "version": "0.0.1",  
    "default_locale": "en",
    "description": "",
    
    "icons": { 
        "16": "images/icon16.png",
        "48": "images/icon48.png",
       "128": "images/icon128.png"
    },
  
    "commands": {
        "dummy-command": {
           "description": "",
           "global": false,
           "suggested_key": {
              "default": ""
           }
        }
    },

    "background": {
      "service_worker": "background.html"
    },

    "content_scripts": [
        {
          "matches": ["https://*/*", "http://*/*"],
          "css": ["css/content_scripts.css"],
          "js": ["js/content_scripts.js"]
        }
      ],

    "permissions": ["tabs"]
  }